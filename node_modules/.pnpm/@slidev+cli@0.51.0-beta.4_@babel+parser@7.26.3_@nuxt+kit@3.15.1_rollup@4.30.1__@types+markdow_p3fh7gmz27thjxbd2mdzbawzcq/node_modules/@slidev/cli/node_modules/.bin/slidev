#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules/@slidev/cli/bin/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules/@slidev/cli/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules/@slidev/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules/@slidev/cli/bin/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules/@slidev/cli/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules/@slidev/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/@slidev+cli@0.51.0-beta.4_@babel+parser@7.26.3_@nuxt+kit@3.15.1_rollup@4.30.1__@types+markdow_p3fh7gmz27thjxbd2mdzbawzcq/node_modules:/home/mehran/Projects/seminar/LiquidHaskell/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../../bin/slidev.mjs" "$@"
fi
